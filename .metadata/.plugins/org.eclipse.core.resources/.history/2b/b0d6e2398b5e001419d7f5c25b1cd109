   public static void setUserAccessTokens(ConfigurationBuilder cb)
   {
     cb.setOAuthConsumerKey("BB9YRzQ42gpUmUPC8iVT6y8Hb");
     cb.setOAuthConsumerSecret("O56dgWaU8eKpyOycWUoVNeONr9SWN9sm7x5Od1SES6LbqNDiKo");
